{
    "name": "lake/lake-admin",
    "description": "The lake-admin is an admin package",
    "license": "Apache-2.0",
    "type": "library",
    "keywords": [
        "lake",
        "admin",
        "lake-admin"
    ],
    "homepage": "https://github.com/deatil/lake-admin",
    "authors": [
        {
            "name": "deatil",
            "email": "deatil@github.com",
            "homepage": "https://github.com/deatil"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0.0",
        "topthink/think-orm": "^2.0",
        "topthink/think-view": "^1.0",
        "topthink/think-multi-app": "^1.0",
        "topthink/think-captcha": "^3.0",
        "overtrue/pinyin": "~4.0",
        "lake/lake": ">=1.0.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "topthink/think-trace":"^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\admin\\": "src/app/admin"
        },
        "files": [
            "src/app/admin/helper.php"
        ],
        "classmap": [
        ]
    },
    "extra": {
        "think":{
            "services":[
                "app\\admin\\boot\\Service"
            ]
        }
    }
}
